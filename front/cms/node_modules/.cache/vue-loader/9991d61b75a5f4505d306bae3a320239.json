{"remainingRequest":"/Users/liuguanghui/Documents/cms_code/my-message/cms/node_modules/vue-loader/lib/index.js??vue-loader-options!/Users/liuguanghui/Documents/cms_code/my-message/cms/src/components/statisticalReportView.vue?vue&type=script&lang=js&","dependencies":[{"path":"/Users/liuguanghui/Documents/cms_code/my-message/cms/src/components/statisticalReportView.vue","mtime":1638803771701},{"path":"/Users/liuguanghui/Documents/cms_code/my-message/cms/node_modules/cache-loader/dist/cjs.js","mtime":499162500000},{"path":"/Users/liuguanghui/Documents/cms_code/my-message/cms/node_modules/babel-loader/lib/index.js","mtime":315532800000},{"path":"/Users/liuguanghui/Documents/cms_code/my-message/cms/node_modules/cache-loader/dist/cjs.js","mtime":499162500000},{"path":"/Users/liuguanghui/Documents/cms_code/my-message/cms/node_modules/vue-loader/lib/index.js","mtime":499162500000}],"contextDependencies":[],"result":[{"type":"Buffer","data":"base64:"},{"version":3,"sources":["statisticalReportView.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AA8MA;AACA;AACA;;AAEA;AACA;AACA;;AAEA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;;AAEA;AACA;;AAEA;AACA;;AAEA;AACA;AACA;;AAEA;;AAEA;AACA;;AAEA;AACA;AACA;;AAEA;;AAEA;;AAEA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;;AAEA;AACA;;AAEA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;;AAEA;AACA;;AAEA;AACA;AACA;;AAEA;AACA;;AAEA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;;AAEA;AACA;;AAEA;;AAEA;AACA;AACA;;AAEA;;AAEA;AACA;;AAEA;AACA;AACA;;AAEA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;;AAEA;AACA;;AAEA;AACA;AACA;;AAEA;AACA;AACA;;AAEA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;;AAEA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;;AAEA;AACA;AACA;AACA;;AAEA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;;AAEA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;;AAEA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;;AAEA;;AAEA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;;AAEA;AACA;AACA;;AAEA;;AAEA;AACA;;AAEA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;;AAEA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;;AAEA;AACA;;AAEA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;;AAEA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;;AAEA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;;AAEA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA","file":"statisticalReportView.vue","sourceRoot":"src/components","sourcesContent":["<template>\n  <v-card class=\"mx-auto\">\n    <v-toolbar dense color=\"#1867C0\">\n      <v-btn icon>\n        <v-icon color=\"white\" @click=\"bt_close\"> mdi-close </v-icon>\n      </v-btn>\n      <v-toolbar-title class=\"body-1 white--text\">统计报告</v-toolbar-title>\n    </v-toolbar>\n    <div class=\"pl-8 pr-8 pt-4\">\n      <span class=\"body-1 grey--text text--darken-3\"> 经销商 </span>\n      <v-row align=\"center\" class=\"mt-6 ml-1 mb-4\">\n        <v-dialog v-model=\"remindDialog\" max-width=\"546\">\n          <template v-slot:activator=\"{ on, attrs }\">\n            <v-btn\n              class=\"white--text\"\n              color=\"#1867C0\"\n              :disabled=\"isDisableButton\"\n              small\n              depress\n              v-bind=\"attrs\"\n              v-on=\"on\"\n            >\n              发送提醒\n            </v-btn>\n          </template>\n          <v-card>\n            <v-card-title>将向以下经销商对应的人员发送提醒</v-card-title>\n            <v-divider></v-divider>\n            <template>\n              <v-data-table\n                :headers=\"reminderheaders\"\n                :items=\"reminderCompanyList\"\n                item-key=\"id\"\n                class=\"mt-8 elevation-1\"\n                hide-default-footer\n                dense\n              >\n              </v-data-table>\n            </template>\n\n            <v-divider></v-divider>\n            <v-card-actions>\n              <v-row justify=\"center\">\n                <v-btn color=\"blue darken-1\" text @click=\"closeReminder\">\n                  取消\n                </v-btn>\n                <v-btn color=\"blue darken-1\" text @click=\"sendReminder\">\n                  发送\n                </v-btn>\n              </v-row>\n            </v-card-actions>\n          </v-card>\n        </v-dialog>\n\n        <v-btn class=\"ml-4\" color=\"#1867C0\" small text depress>\n          <v-icon small>mdi-export</v-icon>\n          回复附件导出\n        </v-btn>\n      </v-row>\n\n      <v-row class=\"pl-4 pt-3 pb-5 pr-4\" justify=\"center\">\n        <v-combobox\n          v-model=\"select_status\"\n          :items=\"items_status\"\n          height=\"36px\"\n          style=\"width: 60px\"\n          class=\"pa-0 mr-4 mr-sm-2\"\n          label=\"状态\"\n          @change=\"selectStatusChange\"\n          outlined\n          dense\n        ></v-combobox>\n\n        <v-combobox\n          v-model=\"select_area\"\n          :items=\"items_area\"\n          height=\"36px\"\n          style=\"width: 60px\"\n          class=\"pa-0 mr-4 mr-sm-2\"\n          label=\"区域\"\n          @change=\"selectAreaChange\"\n          outlined\n          dense\n        ></v-combobox>\n\n        <v-combobox\n          v-model=\"select_type\"\n          :items=\"items_type\"\n          item-value=\"id\"\n          item-text=\"text\"\n          height=\"36px\"\n          style=\"width: 60px\"\n          label=\"类型\"\n          class=\"pa-0 mr-4 mr-sm-2\"\n          @change=\"selectTypeChange\"\n          outlined\n          dense\n        ></v-combobox>\n\n        <v-text-field\n          single-line\n          outlined\n          height=\"36px\"\n          style=\"width: 320px\"\n          class=\"ml-sm-0 ml-md-2 mr-4\"\n          label=\"按照文章编号，标题，正文内容查找\"\n          append-icon=\"mdi-magnify\"\n          hide-details=\"false\"\n          v-model=\"search_sele\"\n          dense\n        ></v-text-field>\n\n        <v-btn\n          outlined\n          class=\"blue--text text--darken-2 mr-4\"\n          height=\"38px\"\n          width=\"97px\"\n          @click=\"bt_search\"\n        >\n          查找\n        </v-btn>\n        <v-btn\n          outlined\n          width=\"97px\"\n          class=\"blue--text text--darken-2\"\n          height=\"38px\"\n          @click=\"bt_reduce\"\n        >\n          还原\n        </v-btn>\n      </v-row>\n\n      <template>\n        <v-data-table\n          :headers=\"headers\"\n          :items=\"desserts_area\"\n          item-key=\"area\"\n          :search=\"search_sele\"\n          class=\"mt-4 elevation-1\"\n          hide-default-footer\n        >\n          <template v-slot:body=\"{ items }\">\n            <tbody>\n              <tr v-for=\"item in items\" :key=\"item.area\" style=\"width: 200px\">\n                <td style=\"text-align: left\">{{ item.area }}</td>\n                <td\n                  style=\"text-align: left\"\n                  @click=\"\n                    clickRow(STATISTICSSTATUS_TYPE_READ, item.area, -1, item)\n                  \"\n                >\n                  {{ item.reading }}\n                </td>\n                <td\n                  style=\"text-align: left\"\n                  @click=\"\n                    clickRow(STATISTICSSTATUS_TYPE_REPLY, item.area, -1, item)\n                  \"\n                >\n                  {{ item.reply }}\n                </td>\n\n                <td\n                  v-for=\"attachment in item.attachDataList\"\n                  :key=\"attachment.id\"\n                  @click=\"\n                    clickRow(\n                      STATISTICSSTATUS_TYPE_ATTACH,\n                      item.area,\n                      attachment.id,\n                      item\n                    )\n                  \"\n                >\n                  {{ attachment.value }}\n                </td>\n              </tr>\n            </tbody>\n          </template>\n        </v-data-table>\n\n        <v-divider class=\"#E4E8EB\"></v-divider>\n\n        <v-dialog\n          v-model=\"dialog_supplier\"\n          fullscreen\n          hide-overlay\n          transition=\"dialog-bottom-transition\"\n        >\n          <SupplierReportView\n            @closedChild=\"closeParent\"\n            :areaparam=\"clickAreaName\"\n            :msgIDParam=\"msgID\"\n            :openStatus=\"dialog_supplier\"\n            :readStatusParam=\"selectReadValue\"\n            :statisticsStatusParam=\"statisticsStatus\"\n            :attachmentIdParam=\"attachmentId\"\n            :selectTypeValueParam=\"selectTypeValue\"\n          ></SupplierReportView>\n        </v-dialog>\n      </template>\n    </div>\n  </v-card>\n</template>\n\n<script>\nimport SupplierReportView from \"@/components/statisticalReportSupplierView.vue\";\nimport { getDealerReport, sendReportReminder } from \"@/api/getData.js\";\nimport { composeStatisticsShowText } from \"@/utils/stringUtils.js\";\n\nexport default {\n  name: \"statisticalReportView\",\n  props: [\"msgIdParam\"],\n\n  components: {\n    SupplierReportView,\n  },\n\n  data() {\n    return {\n      //状态：已读、未读\n      STATUS_READ: 0,\n      STATUS_UNREAD: 1,\n\n      //统计查询类型 0: 阅读类型查询 1: 附件类型查询 2: 回复类型查询\n      STATISTICSSTATUS_TYPE_READ: 0,\n      STATISTICSSTATUS_TYPE_ATTACH: 1,\n      STATISTICSSTATUS_TYPE_REPLY: 2,\n\n      DEALERTYPE: [\n        { text: \"全部\", id: \"\" },\n        { text: \"4S店\", id: \"1\" },\n        { text: \"独立经销商\", id: \"0\" },\n        { text: \"二级网点\", id: \"2\" },\n      ],\n\n      remindDialog: false,\n      search_sele: \"\",\n      selected_area: [],\n      expanded: [],\n      clickAreaName: \"\",\n      statisticsStatus: 0,\n      attachmentId: 0,\n\n      reminderCompanyList: [],\n      unSelectedReminderUserId: [],\n      unSelectReminderCompanyIds: [],\n\n      dialog_supplier: false,\n      msgID: this.msgIdParam,\n\n      headers: [],\n      desserts_area: [],\n\n      items_status: [\"已读\", \"未读\"],\n      items_area: [],\n      items_type: [],\n\n      isDisableButton: true,\n\n      selectReadValue: this.STATUS_READ,\n      selectTypeValue: \"\",\n\n      select_status: \"已读\",\n      select_area: \"全部\",\n      select_type: [],\n\n      allUsers: [],\n\n      selectComanyInfoMapInfo: new Map(),\n\n      reminderheaders: [\n        { text: \"区域\", value: \"area\" },\n        { text: \"经销商\", value: \"companyName\" },\n      ],\n    };\n  },\n\n  mounted() {\n    console.log(\"mounted --- msgID:\" + this.msgID);\n    this.items_type = this.DEALERTYPE;\n    this.select_type = { text: \"全部\", id: \"\" };\n    this.selectReadValue = this.STATUS_READ;\n\n    this.getDealerReportService();\n  },\n\n  methods: {\n    getDealerReportService() {\n      this.resetData();\n\n      getDealerReport({\n        area: this.select_area == \"全部\" ? \"\" : this.select_area,\n        msgId: this.msgID,\n        level: 0,\n        readStatus: this.selectReadValue,\n        dealerType: this.selectTypeValue,\n        searchContent: this.search_sele,\n      })\n        .then((res) => {\n          console.log(\"getDealerReport receive:\" + JSON.stringify(res));\n\n          if (res.data == null || res.data.length == 0) {\n            console.log(\"data size is 0\");\n            return;\n          }\n\n          this.headers = this.initHeaderData(res.data);\n          this.desserts_area = this.initBodyData(res.data);\n\n          if (this.items_area.length == 0) {\n            this.items_area = this.initItemsAread(res.data);\n          }\n\n          //初始化提醒人员信息\n          this.initSendReminderCompanyList();\n\n        })\n        .catch((res) => {\n          console.log(res);\n        });\n    },\n\n    resetData() {\n      this.headers = [];\n      this.desserts_area = [];\n      this.allUsers = [];\n    },\n\n    //构建单个header头信息\n    buildHeaderBean(text, value) {\n      let headerBean = {};\n      headerBean.text = text;\n      headerBean.value = value;\n      headerBean.width = \"10rem\";\n      headerBean.align = \"left\";\n\n      return headerBean;\n    },\n\n    initItemsAread(data) {\n      let resutData = [];\n\n      resutData.push(\"全部\");\n\n      data.forEach((statisticsReport) => {\n        resutData.push(statisticsReport.area);\n      });\n\n      console.log(\"initItemsAread:\" + JSON.stringify(resutData));\n\n      return resutData;\n    },\n\n    initSendReminderCompanyList() {\n      let reminderCompanyMap = new Map();\n      this.reminderCompanyList = [];\n\n      this.allUsers.forEach((userInfo) => {\n        let selectCompanyList = this.selectComanyInfoMapInfo.get(\n          userInfo.companyCode\n        );\n\n        if (\n          selectCompanyList == undefined ||\n          selectCompanyList.indexOf(userInfo.userId) >= 0\n        ) {\n          let reminderCompanyInfo = {};\n\n          if (reminderCompanyMap.has(userInfo.companyCode) == false) {\n            reminderCompanyInfo.area = userInfo.area;\n            reminderCompanyInfo.companyName = userInfo.companyName;\n            reminderCompanyInfo.companyCode = userInfo.companyCode;\n\n            reminderCompanyMap.set(userInfo.companyCode, reminderCompanyInfo);\n            this.reminderCompanyList.push(reminderCompanyInfo);\n          }\n        }\n      });\n\n      console.log('reminderCompanyList:'+JSON.stringify(this.reminderCompanyList));\n    },\n\n    //初始化Header的数据\n    initHeaderData(data) {\n      let headerDataArray = [];\n\n      headerDataArray.push(this.buildHeaderBean(\"名称\", \"area\"));\n      headerDataArray.push(this.buildHeaderBean(\"阅读统计\", \"reading\"));\n      headerDataArray.push(this.buildHeaderBean(\"回复统计\", \"reply\"));\n\n      if (data[0].attachmentStatistics.length == 0) {\n        console.log(\"initHeaderData:attachmentStatistics is 0\");\n        return headerDataArray;\n      }\n\n      data[0].attachmentStatistics.forEach((attachmentStatistics) => {\n        let attachmentName = attachmentStatistics.attachmentName.slice(\n          attachmentStatistics.attachmentName.lastIndexOf(\"/\") + 1\n        );\n\n        headerDataArray.push(\n          this.buildHeaderBean(\n            \"下载统计(\" + attachmentName + \")\",\n            attachmentStatistics.attachmentId\n          )\n        );\n      });\n\n      console.log(\"headerDataArray:\" + JSON.stringify(headerDataArray));\n\n      return headerDataArray;\n    },\n\n    /**\n     * 组装表格内容\n     */\n    initBodyData(data) {\n      let dataArray = [];\n      let totalinfo = {};\n      let totalAttachmentStatistics = [];\n\n      totalinfo.area = \"合计\";\n\n      let readStatisticsCount = 0,\n        readStatisticsTotalCount = 0;\n      let feedbackStatisticsCount = 0,\n        feedbackStatisticsTotalCount = 0;\n\n      data.forEach((statisticsReport) => {\n        let bodyBean = {};\n\n        bodyBean.area = statisticsReport.area;\n        bodyBean.reading = composeStatisticsShowText(\n          statisticsReport.readStatistics.statisticsCount,\n          statisticsReport.readStatistics.totalCount,\n          statisticsReport.readStatistics.percentage\n        );\n        bodyBean.reply = composeStatisticsShowText(\n          statisticsReport.feedBackStatistics.statisticsCount,\n          statisticsReport.feedBackStatistics.totalCount,\n          statisticsReport.feedBackStatistics.percentage\n        );\n\n        readStatisticsCount += statisticsReport.readStatistics.statisticsCount;\n        readStatisticsTotalCount += statisticsReport.readStatistics.totalCount;\n\n        feedbackStatisticsCount +=\n          statisticsReport.feedBackStatistics.statisticsCount;\n        feedbackStatisticsTotalCount +=\n          statisticsReport.feedBackStatistics.totalCount;\n\n        /**\n         * 组装Users\n         */\n        this.composeUserData(statisticsReport.users);\n\n        //附件下载统计\n        let attachDataList = [];\n\n        statisticsReport.attachmentStatistics.forEach(\n          (attachmentStatistics, index) => {\n            let attachData = {};\n            if (totalAttachmentStatistics[index] == undefined) {\n              totalAttachmentStatistics[index] = {\n                id: \"\",\n                statisticsCount: 0,\n                totalCount: 0,\n              };\n            }\n\n            attachData.id = attachmentStatistics.attachmentId;\n            attachData.value = composeStatisticsShowText(\n              attachmentStatistics.dealerStatistics.statisticsCount,\n              attachmentStatistics.dealerStatistics.totalCount,\n              attachmentStatistics.dealerStatistics.percentage\n            );\n\n            totalAttachmentStatistics[index].id =\n              attachmentStatistics.attachmentId;\n            totalAttachmentStatistics[index].statisticsCount +=\n              attachmentStatistics.dealerStatistics.statisticsCount;\n            totalAttachmentStatistics[index].totalCount +=\n              attachmentStatistics.dealerStatistics.totalCount;\n\n            attachDataList.push(attachData);\n          }\n        );\n\n        bodyBean.attachDataList = attachDataList;\n\n        dataArray.push(bodyBean);\n      });\n\n      totalinfo.reading = composeStatisticsShowText(\n        readStatisticsCount,\n        readStatisticsTotalCount,\n        (readStatisticsCount * 100) / readStatisticsTotalCount\n      );\n      totalinfo.reply = composeStatisticsShowText(\n        feedbackStatisticsCount,\n        feedbackStatisticsTotalCount,\n        (feedbackStatisticsCount * 100) / feedbackStatisticsTotalCount\n      );\n\n      totalinfo.attachDataList = [];\n      totalAttachmentStatistics.forEach((attachmentStatistics) => {\n        let attachData = {};\n\n        attachData.id = attachmentStatistics.attachmentId;\n        attachData.value = composeStatisticsShowText(\n          attachmentStatistics.statisticsCount,\n          attachmentStatistics.totalCount,\n          (attachmentStatistics.statisticsCount * 100) /\n            attachmentStatistics.totalCount\n        );\n\n        totalinfo.attachDataList.push(attachData);\n      });\n\n      console.log(\n        \"totalAttachmentStatistics:\" + JSON.stringify(totalAttachmentStatistics)\n      );\n\n      dataArray.unshift(totalinfo);\n\n      return dataArray;\n    },\n\n    /**\n     * 获得需要提醒的用户信息\n     */\n    getReminderUserIds() {\n      let userIds = [];\n\n      for (let [key, value] of this.selectComanyInfoMapInfo.entries()) {\n        console.log(\"getReminderUserIds:selectCompanyInfo:\" + value, key);\n      }\n\n      console.log(\n        \"getReminderUserIds:allUsers:\" + JSON.stringify(this.allUsers)\n      );\n\n      this.allUsers.forEach((userinfo) => {\n        let userInfo={};\n        let selectUserList = this.selectComanyInfoMapInfo.get(\n          userinfo.companyCode\n        );\n\n        if (\n          selectUserList == undefined ||\n          selectUserList.indexOf(userinfo.userId) >= 0\n        ) {\n          userInfo.userId = userinfo.userId;\n          userIds.push(userInfo);\n        }\n      });\n\n      console.log(\"getReminderUserIds:userIds:\" + JSON.stringify(userIds));\n\n      return userIds;\n    },\n\n    composeUserData(users) {\n      users.forEach((user) => {\n        let reminderUserInfo = {};\n\n        reminderUserInfo.area = user.dealerObj.area;\n        reminderUserInfo.name = user.name;\n        reminderUserInfo.userId = user.userId;\n        reminderUserInfo.companyName = user.dealerObj.companyName;\n        reminderUserInfo.companyCode = user.dealerObj.companyCode;\n\n        this.allUsers.push(reminderUserInfo);\n      });\n    },\n\n    closeReminder() {\n      console.log(\"......关闭提醒按钮.......\");\n      this.remindDialog = false;\n    },\n\n    //关闭\n    bt_close() {\n      console.log(\"......关闭按钮.......\");\n      this.$emit(\"closedChild\", false);\n    },\n    //关闭dialog\n    closeParent(selectComanyInfoMapInfo) {\n      console.log(\"-----------\" + this.nowDialog);\n      this.dialog_supplier = false;\n\n      this.selectComanyInfoMapInfo.clear();\n      for (let [key, value] of selectComanyInfoMapInfo.entries()) {\n        console.log(\n          \"selectComanyInfoMapInfo put key:\" + key + \"  value:\" + value\n        );\n\n        this.selectComanyInfoMapInfo.set(key, value);\n      }\n\n      this.initSendReminderCompanyList();\n    },\n    //发送提醒\n    sendReminder() {\n      console.log(\"......发送提醒.......\");\n      this.remindDialog = false;\n\n      sendReportReminder({\n        msgId: this.msgID,\n        title: \"\",\n        operator: \"\",\n        operation: 4,\n        userIds: this.getReminderUserIds(),\n      });\n    },\n    //查找\n    bt_search() {\n      console.log(\"......查找.......:\" + this.search_sele);\n      // if (this.search_sele != \"\") {\n      this.getDealerReportService();\n      // }\n    },\n    //还原\n    bt_reduce() {\n      console.log(\"......还原.......\");\n      this.search_sele = \"\";\n      (this.select_status = \"已读\"),\n        (this.select_type = { text: \"全部\", id: \"\" });\n      this.selectReadValue = this.STATUS_READ;\n      this.select_area = \"全部\";\n\n      this.getDealerReportService();\n    },\n    //状态选择的值改变\n    selectStatusChange() {\n      console.log(\"....状态选择.... :\" + this.select_status);\n      if (this.select_status == \"未读\") {\n        this.selectReadValue = this.STATUS_UNREAD;\n        this.isDisableButton = false;\n      } else {\n        this.selectReadValue = this.STATUS_READ;\n        this.isDisableButton = true;\n      }\n\n      this.getDealerReportService();\n    },\n    //区域选择的值改变\n    selectAreaChange() {\n      console.log(\"....区域选择.... :\" + this.select_area);\n\n      this.getDealerReportService();\n    },\n    //类型选择的值改变\n    selectTypeChange() {\n      this.selectTypeValue = this.select_type.id;\n\n      console.log(\"....类型选择.... selectTypeValue:\" + this.selectTypeValue);\n\n      this.getDealerReportService();\n    },\n    //当table row click时触发\n    clickRow: function (type, area, attachId) {\n      if (\"合计\" == area) {\n        this.clickAreaName = \"\";\n      } else {\n        this.clickAreaName = area;\n      }\n      this.statisticsStatus = type;\n      this.attachmentId = attachId;\n\n      console.log(\n        \"....rowClick.... statisticsStatus:\" +\n          this.statisticsStatus +\n          \"  clickAreaName:\" +\n          this.clickAreaName +\n          \" attachmentId:\" +\n          this.attachmentId\n      );\n\n      this.dialog_supplier = true;\n    },\n  },\n};\n</script>\n\n<style lang=\"scss\" scoped>\n</style>"]}]}
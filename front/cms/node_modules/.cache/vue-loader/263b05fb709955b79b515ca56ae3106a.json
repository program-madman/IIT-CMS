-vote-outline </v-icon>\n        <span class=\"vote-title ml-2\">{{ vote.topic }}</span>\n      </v-row>\n      <v-row>\n        <v-radio-group v-model=\"radioGroup\" class=\"mt-2\">\n          <v-radio\n            v-for=\"(item, raindex) in vote.voteOptions\"\n            :key=\"raindex\"\n            :ripple=\"false\"\n            :label=\"item.value\"\n            on-icon=\"mdi-check-circle\"\n            class=\"vote-radio caption\"\n          ></v-radio>\n        </v-radio-group>\n      </v-row>\n      <v-row class=\"mt-0 pa-0\">\n        <v-btn\n          depressed\n          color=\"#1867C0\"\n          class=\"white--text\"\n          width=\"90\"\n          height=\"27\"\n        >\n          投票\n        </v-btn>\n        <v-spacer></v-spacer>\n\n        <v-btn\n          depressed\n          plain\n          text\n          color=\"#1867c0ff\"\n          :ripple=\"false\"\n          height=\"27px\"\n          min-width=\"24px\"\n          class=\"pa-0 ma-0 mr-2\"\n          @click=\"onUpdate\"\n          v-if=\"!isPreview\"\n        >\n          修改\n        </v-btn>\n\n        <v-btn\n          depressed\n          plain\n          text\n          color=\"#1867c0ff\"\n          :ripple=\"false\"\n          height=\"27px\"\n          min-width=\"24px\"\n          class=\"pa-0 ma-0\"\n          @click=\"onDelete\"\n          v-if=\"!isPreview\"\n        >\n          删除\n        </v-btn>\n      </v-row>\n    </v-container>\n  </v-sheet>\n</template>\n<script>\nexport default {\n  props: {\n    vote: {\n      type: [String, Object],\n      default: \"\",\n    },\n    isPreview:{\n      type: Boolean,\n      default: false,\n    }\n  },\n  data() {\n    return {\n      radioGroup: 0,\n    };\n  },\n\n  methods: {\n    onDelete() {\n      // 父组件 回调传参\n      this.$emit(\"onDelete\");\n    },\n    onUpdate() {\n      this.$emit(\"onUpdate\");\n    },\n  },\n};\n</script>\n<style>\n.vote .vote-title {\n  color: #333333ff;\n  font-family: PingFang SC;\n  font-weight: regular;\n  font-size: 14px;\n  line-height: 20px;\n  letter-spacing: 0px;\n  text-align: left;\n  align-self: center;\n}\n\n.vote-radio .v-label {\n  color: #333333ff;\n  font-family: PingFang SC;\n  font-weight: regular;\n  font-size: 12px;\n  line-height: 18px;\n  letter-spacing: 0px;\n  text-align: left;\n}\n.vote .vote-radio {\n  height: 14px;\n}\n.vote .v-icon.v-icon {\n  font-size: 14px;\n}\n.vote .v-icon__svg,\n.vote .v-input--selection-controls__input {\n  width: 14px;\n  height: 14px;\n}\n\n.vote .v-btn.v-size--default {\n  font-family: PingFang SC;\n  font-weight: regular;\n  font-size: 12px;\n  line-height: normal;\n  letter-spacing: 0px;\n  text-align: left;\n}\n</style>\n"]}]}
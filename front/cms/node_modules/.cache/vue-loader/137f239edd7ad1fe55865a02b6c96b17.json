               this.changeList(this.ch_child_int_2, false);\n                    }\n                }\n            },\n\n            //筛选一组条件\n            changeList(arry, able) {\n                arry.forEach(function (item, index) {\n                    arry[index].isCheck = able;\n                    arry[index].isable = !able;\n                });\n            },\n\n            getDealerRangeList(msgId){\n                getDealerRangeList(msgId).then(res => {\n                    for(let i=0;i<res.length;i++){\n                        let dealerObj = res[i];\n                        console.log(\"dealerObj\");\n                        console.log(dealerObj);\n                        console.log(\"dealerObj\");\n                    }\n                    this.ch_area_supp = [{\n                        id: 11,\n                        isCheck: false,\n                        lable: \"集团物流部\",\n                        isable: true,\n                    },\n                    {\n                        id: 12,\n                        isCheck: false,\n                        lable: \"中央市场部\",\n                        isable: true,\n                    }];\n                    this.ch_com_supp = [{\n                        id: 11,\n                        isCheck: false,\n                        lable: \"集团物流部\",\n                        isable: true,\n                    },\n                    {\n                        id: 12,\n                        isCheck: false,\n                        lable: \"中央市场部\",\n                        isable: true,\n                    }];\n                    this.ch_child_supp_1 = [{\n                        id: 11,\n                        isCheck: false,\n                        lable: \"集团物流部\",\n                        isable: true,\n                    },\n                    {\n                        id: 12,\n                        isCheck: false,\n                        lable: \"中央市场部\",\n                        isable: true,\n                    }];\n                    this.ch_child_supp_2 = [{\n                        id: 11,\n                        isCheck: false,\n                        lable: \"集团物流部\",\n                        isable: true,\n                    },\n                    {\n                        id: 12,\n                        isCheck: false,\n                        lable: \"中央市场部\",\n                        isable: true,\n                    }];\n                    this.ch_child_int_1 = [{\n                        id: 11,\n                        isCheck: false,\n                        lable: \"集团物流部\",\n                        isable: true,\n                    },\n                    {\n                        id: 12,\n                        isCheck: false,\n                        lable: \"中央市场部\",\n                        isable: true,\n                    }];\n\n                }).catch(res => {\n                    console.log(res);\n                })\n            },\n\n            //选中后变动child\n            selectChild(item,child,grandson,isarea){   \n                //如果grandon true 标示change 第一栏\n                if(grandson){\n                    if(isarea){\n                        let father = this.ch_area_supp.concat(this.ch_com_supp);\n                        this.ch_all_supp = true;\n                        for(let n=0;n<father.length;n++){\n                            if(!father[n].isCheck){\n                                this.ch_all_supp = false;\n                                break;\n                            }\n                        }\n                    }else{\n                        let father = this.ch_int;\n                        this.ch_all_int = true;\n                        for(let n=0;n<father.length;n++){\n                            if(!father[n].isCheck){\n                                this.ch_all_int = false;\n                                break;\n                            }\n                        }\n\n                    }\n                }\n\n                for(let i=0;i<child.length;i++){\n                    if(item.id == parseInt(child[i].id/10)){\n                        child[i].isable = !item.isCheck;\n                        child[i].isCheck = item.isCheck;\n                        if(grandson){\n                            this.selectChild(child[i],grandson);\n                        }\n                    }\n                }\n            },\n        },\n    };\n</script>"]}]}
,KApEA;AAqEA;AACA,IAAA,WAtEA,yBAsEA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,gBAAA,KAAA,SAAA;AACA,WAAA,eAAA,GAAA,KAAA;AACA,KAzEA;AA0EA;AACA,IAAA,UA3EA,wBA2EA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,mBAAA;AACA,WAAA,YAAA,GAAA,KAAA;AACA,KA9EA;AA+EA;AACA,IAAA,SAhFA,uBAgFA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,iBAAA;AACA,KAlFA;AAmFA;AACA,IAAA,SApFA,uBAoFA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,iBAAA;AACA,WAAA,WAAA,GAAA,EAAA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,KAAA,WAAA;AACA,KAxFA;AAyFA;AACA,IAAA,kBA1FA,gCA0FA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,mBAAA,KAAA,aAAA;AACA,KA5FA;AA6FA;AACA,IAAA,gBA9FA,8BA8FA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,mBAAA,KAAA,WAAA;AACA,KAhGA;AAiGA;AACA,IAAA,gBAlGA,8BAkGA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,mBAAA,KAAA,WAAA;AACA,KApGA;AAqGA;AACA,IAAA,QAAA,EAAA,kBAAA,IAAA,EAAA,EAAA,EAAA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,2BAAA,IAAA,GAAA,OAAA,GAAA,EAAA;AACA,WAAA,eAAA,GAAA,IAAA;AACA,WAAA,eAAA,GAAA,EAAA;AACA;AA1GA;AAjCA,CAAA","sourcesContent":["<template>\n  <v-card class=\"mx-auto\">\n    <v-toolbar dense color=\"#1867C0\">\n      <v-btn icon>\n        <v-icon color=\"white\" @click=\"bt_close\"> mdi-close </v-icon>\n      </v-btn>\n      <v-toolbar-title class=\"body-1 white--text\">统计报告</v-toolbar-title>\n    </v-toolbar>\n    <div class=\"pl-8 pr-8 pt-4\">\n      <span class=\"body-1 grey--text text--darken-3\"> 经销商 </span>\n      <v-row align=\"center\" class=\"mt-6\">\n        <v-dialog v-model=\"remindDialog\" max-width=\"546\">\n          <template v-slot:activator=\"{ on, attrs }\">\n            <v-btn\n              class=\"white--text\"\n              color=\"#1867C0\"\n              small\n              depress\n              v-bind=\"attrs\"\n              v-on=\"on\"\n            >\n              发送提醒\n            </v-btn>\n          </template>\n          <v-card>\n            <v-card-title justify-center>发送提醒</v-card-title>\n            <v-card-text>确认向已勾选经销商发送提醒吗？</v-card-text>\n            <v-card-actions>\n              <v-btn color=\"black darken-1\" text @click=\"remindDialog = false\">\n                取消\n              </v-btn>\n\n              <v-btn color=\"blue darken-1\" text @click=\"sendRemind\">\n                发送\n              </v-btn>\n            </v-card-actions>\n          </v-card>\n        </v-dialog>\n\n        <v-btn class=\"ml-4\" color=\"#1867C0\" small text depress>\n          <v-icon small>mdi-export</v-icon>\n          回复附件导出\n        </v-btn>\n      </v-row>\n\n      <v-row class=\"pl-4 pt-3 pb-5 pr-4\">\n        <v-col\n          cols=\"12\"\n          sm=\"12\"\n          md=\"7\"\n          class=\"pa-0 pt-4\"\n          style=\"display: inline-flex\"\n        >\n          <v-combobox\n            v-model=\"select_status\"\n            :items=\"items_status\"\n            height=\"36px\"\n            width=\"195px\"\n            style=\"width: 195px; min-width: 195px\"\n            class=\"pa-0 mr-4 mr-sm-2\"\n            label=\"状态\"\n            @change=\"selectStatusChange\"\n            outlined\n            dense\n          ></v-combobox>\n\n          <v-combobox\n            v-model=\"select_area\"\n            :items=\"items_area\"\n            height=\"36px\"\n            style=\"width: 195px\"\n            width=\"195px\"\n            class=\"pa-0 mr-4 mr-sm-2\"\n            label=\"区域\"\n            @change=\"selectAreaChange\"\n            multiple\n            outlined\n            dense\n          ></v-combobox>\n\n          <v-combobox\n            v-model=\"select_type\"\n            :items=\"items_type\"\n            height=\"36px\"\n            width=\"195px\"\n            style=\"width: 195px\"\n            label=\"类型\"\n            class=\"pa-0 mr-4 mr-sm-2\"\n            @change=\"selectTypeChange\"\n            multiple\n            outlined\n            dense\n          ></v-combobox>\n        </v-col>\n\n        <v-col\n          cols=\"12\"\n          sm=\"12\"\n          md=\"3\"\n          class=\"pa-0 pt-4\"\n          style=\"display: inline-flex\"\n        >\n          <v-text-field\n            single-line\n            outlined\n            height=\"36px\"\n            class=\"ml-sm-0 ml-md-2 mr-4\"\n            label=\"按照文章编号，标题，正文内容查找\"\n            append-icon=\"mdi-magnify\"\n            hide-details=\"false\"\n            v-model=\"search_sele\"\n            dense\n          ></v-text-field>\n\n          <v-btn\n            outlined\n            class=\"grey--text text--darken-2 mr-4\"\n            height=\"36px\"\n            width=\"97px\"\n            @click=\"bt_search\"\n          >\n            查找\n          </v-btn>\n          <v-btn\n            outlined\n            width=\"97px\"\n            class=\"grey--text text--darken-2\"\n            height=\"36px\"\n            @click=\"bt_reduce\"\n          >\n            还原\n          </v-btn>\n        </v-col>\n      </v-row>\n\n      <template>\n        <v-data-table\n          :headers=\"headers\"\n          :items=\"desserts_area\"\n          item-key=\"area\"\n          :search=\"search_sele\"\n          class=\"mt-8 elevation-1\"\n          hide-default-footer\n          dense\n        >\n      \n          <template v-slot:body=\"{ items }\">\n            <tbody>\n              <tr\n                v-for=\"item in items\"\n                :key=\"item.area\"\n                style=\"width:200px;\"\n              >\n                <td style=\"text-align:left;\">{{ item.area }}</td>\n                <td style=\"text-align: left\" @click=\"clickRow(0, item.area)\">\n                  {{ item.reading }}\n                </td>\n                <td style=\"text-align: left\" @click=\"clickRow(1, item.area)\">\n                  {{ item.reply }}\n                </td>\n\n                <td v-for=\"attachment in item.attachDataList\" :key=\"attachment.id\">\n                  {{ attachment.value }}\n                </td>\n                \n              </tr>\n            </tbody>\n          </template>\n        </v-data-table>\n\n        <v-divider class=\"#E4E8EB\"></v-divider>\n\n        <v-dialog\n          v-model=\"dialog_supplier\"\n          fullscreen\n          hide-overlay\n          transition=\"dialog-bottom-transition\"\n        >\n          <SupplierReportView @closedChild=\"closeParent\" :suppilerId=\"clickSuppilerId\"></SupplierReportView>\n        </v-dialog>\n      </template>\n    </div>\n  </v-card>\n</template>\n\n<script>\nimport SupplierReportView from \"@/components/statisticalReportSupplierView.vue\";\n\nexport default {\n  name: \"statisticalReportView\",\n  components: {\n    SupplierReportView,\n  },\n\n  data() {\n    return {\n      remindDialog: false,\n      search_sele: \"\",\n      selected_area: [],\n      expanded: [],\n      clickSuppilerId: \"\",\n      dialog_supplier: false,\n\n      headers: [],\n      desserts_area: [],\n\n      items_status: [\"已读\", \"未读\"],\n      items_area: [\"北区\", \"中区\", \"南区\"],\n      items_type: [\"4S店\", \"独立售后\", \"二级网点\"],\n\n      select_status: [\"未读\"],\n      select_area: [\"北区\", \"中区\", \"南区\"],\n      select_type: [\"4S店\", \"独立售后\", \"二级网点\"],\n    };\n  },\n\n  mounted() {\n    this.headers = this.initHeaderData();\n    this.desserts_area = this.initBodyData();\n  },\n\n  methods: {\n    //构建单个header头信息\n    buildHeaderBean(text, value) {\n      let headerBean = {};\n      headerBean.text = text;\n      headerBean.value = value;\n\n      return headerBean;\n    },\n\n    //初始化Header的数据\n    initHeaderData() {\n      let headerDataArray = [];\n      headerDataArray.push(this.buildHeaderBean(\"名称\", \"area\"));\n      headerDataArray.push(this.buildHeaderBean(\"阅读统计\", \"reading\"));\n      headerDataArray.push(this.buildHeaderBean(\"回复统计\", \"reply\"));\n\n      for (let i = 0; i < 4; i++) {\n        headerDataArray.push(this.buildHeaderBean(\"下载统计(附件\" + (i + 1) + \")\", \"download\"+i));\n      }\n\n      return headerDataArray;\n    },\n\n    initBodyData() {\n      let dataArray = [];\n\n      for (let i = 0; i < 4; i++) {\n        let bodyBean = {};\n\n        if (i == 0) {\n          bodyBean.area = \"合计\";\n        } else if (i == 1) {\n          bodyBean.area = \"北区\";\n        } else if (i == 2) {\n          bodyBean.area = \"南区\";\n        } else {\n          bodyBean.area = \"中区\";\n        }\n\n        bodyBean.reading = i + 1 + \"/12  \" + (20 + i * 3) + \"%\";\n        bodyBean.reply = i + 1 + \"/12  \" + (20 + i * 3) + \"%\";\n\n        let attachDataList = [];\n\n        for (let index = 0; index < 4; index++) {\n          let attachData = {};\n\n          attachData.id = index;\n          attachData.value = index + 1 + \"/12  \" + (20 + index * 3) + \"%\";\n\n          attachDataList[index] = attachData;\n        }\n\n        bodyBean.attachDataList = attachDataList;\n\n        dataArray[i] = bodyBean;\n\n        console.log(\"initBodyData \" + i + \":\" + JSON.stringify(bodyBean));\n      }\n\n      return dataArray;\n    },\n\n    //关闭\n    bt_close() {\n      console.log(\"......关闭按钮.......\");\n      this.$emit(\"closedChild\", false);\n    },\n    //关闭dialog\n    closeParent() {\n      console.log(\"-----------\" + this.nowDialog);\n      this.dialog_supplier = false;\n    },\n    //发送提醒\n    sendRemind() {\n      console.log(\"......发送提醒.......\");\n      this.remindDialog = false;\n    },\n    //查找\n    bt_search() {\n      console.log(\"......查找.......\");\n    },\n    //还原\n    bt_reduce() {\n      console.log(\"......还原.......\");\n      this.search_sele = \"\";\n      console.log(this.search_sele);\n    },\n    //状态选择的值改变\n    selectStatusChange() {\n      console.log(\"....状态选择.... :\" + this.select_status);\n    },\n    //区域选择的值改变\n    selectAreaChange() {\n      console.log(\"....区域选择.... :\" + this.select_area);\n    },\n    //类型选择的值改变\n    selectTypeChange() {\n      console.log(\"....类型选择.... :\" + this.select_type);\n    },\n    //当table row click时触发\n    clickRow: function (type, id) {\n      console.log(\"....rowClick.... type:\" + type + \"  id:\" + id);\n      this.dialog_supplier = true;\n      this.clickSuppilerId = id;\n    },\n  },\n};\n</script>\n\n<style lang=\"scss\" scoped>\n</style>"],"sourceRoot":"src/components"}]}
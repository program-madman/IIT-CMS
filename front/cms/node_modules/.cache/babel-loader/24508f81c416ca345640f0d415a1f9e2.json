,GAAA,KAAA;AACA,QAAA,KAAA,CAAA,WAAA,GAAA,KAAA;AACA,OAhDA;AAiDA,KAtEA;AAuEA,IAAA,qBAvEA,mCAuEA;AACA,UAAA,KAAA,IAAA,CAAA,OAAA,IAAA,CAAA,EAAA;AACA,aAAA,OAAA,GAAA,KAAA;AACA,aAAA,aAAA,GAAA,IAAA;AACA;AACA,KA5EA;AA8EA;AACA,IAAA,QA/EA,oBA+EA,EA/EA,EA+EA;AACA,WAAA,OAAA,CAAA,IAAA,CAAA;AACA,QAAA,IAAA,EAAA,eADA;AAEA,QAAA,KAAA,EAAA;AACA,UAAA,EAAA,EAAA,UAAA,CAAA,EAAA,GAAA,EAAA,CADA;AAEA,UAAA,WAAA,EAAA;AAFA;AAFA,OAAA;AAOA,KAvFA;AAwFA;AACA,IAAA,MAzFA,kBAyFA,EAzFA,EAyFA;AACA,WAAA,OAAA,CAAA,IAAA,CAAA;AACA,QAAA,IAAA,EAAA,gBADA;AAEA,QAAA,MAAA,EAAA;AAAA,UAAA,EAAA,EAAA;AAAA;AAFA,OAAA;AAIA,KA9FA;AA+FA;AACA,IAAA,OAhGA,mBAgGA,KAhGA,EAgGA;AAAA;;AACA;AACA,UAAA,IAAA,GAAA,KAAA,KAAA,CAAA,KAAA,CAAA;AACA,UAAA,IAAA,GAAA,MAAA,CAAA,IAAA,CAAA,cAAA,CAAA;AACA,UAAA,MAAA,GAAA;AACA,QAAA,KAAA,EAAA,MAAA,CAAA,IAAA,CAAA,EAAA,CADA;AAEA,QAAA,SAAA,EAAA;AAFA,OAAA;AAIA,MAAA,OAAA,CAAA,KAAA,CAAA,iBAAA,IAAA;AACA,MAAA,IAAA,CAAA,cAAA,GAAA,IAAA;AAEA,MAAA,mBAAA,CAAA,MAAA,CAAA,CACA,IADA,CACA,UAAA,GAAA,EAAA;AACA,QAAA,OAAA,CAAA,KAAA,CAAA,WAAA,GAAA;AACA,QAAA,MAAA,CAAA,KAAA,CAAA,KAAA,EAAA,QAAA,GAAA,IAAA;AACA,OAJA,EAKA,KALA,GAMA,OANA,CAMA;AAAA,eAAA,MAAA,CAAA,KAAA,CAAA,KAAA,EAAA,IAAA,GAAA,KAAA;AAAA,OANA;AAOA,KAlHA;AAoHA,IAAA,QAAA,EAAA,YAAA,CAAA,UAAA,IAAA,EAAA,KAAA,EAAA;AAAA;;AACA,WAAA,KAAA,CAAA,KAAA,EAAA,KAAA,GAAA,KAAA;AACA,MAAA,eAAA,CAAA,IAAA,CAAA,EAAA,CAAA,CACA,IADA,CACA,UAAA,GAAA,EAAA;AACA,YAAA,CAAA,GAAA,IAAA,OAAA,GAAA,IAAA,WAAA,IAAA,GAAA,CAAA,IAAA,IAAA,CAAA,EAAA;;AACA,YAAA,CAAA,GAAA,MAAA,CAAA,KAAA,CAAA,OAAA,CAAA,IAAA,CAAA;;AACA,YAAA,CAAA,IAAA,CAAA,EAAA;AACA,UAAA,MAAA,CAAA,KAAA,CAAA,MAAA,CAAA,CAAA,EAAA,CAAA;AACA;AACA,OAPA,EAQA,KARA;AASA,KAXA;AApHA,GAvBA;AAwJA;AACA,EAAA,OAAA,EAAA;AACA,IAAA,OADA,mBACA,IADA,EACA,MADA,EACA;AAAA,UAAA,MAAA,uEAAA,KAAA;AACA,UAAA,CAAA,IAAA,EAAA;AACA,MAAA,IAAA,GAAA,IAAA,CAAA,OAAA,CAAA,UAAA,EAAA,EAAA,CAAA;;AACA,UAAA,IAAA,CAAA,MAAA,IAAA,MAAA,EAAA;AACA,eAAA,IAAA;AACA;;AACA,aAAA,CAAA,IAAA,GAAA,EAAA,EAAA,OAAA,CAAA,KAAA,EAAA,OAAA,EAAA,SAAA,CAAA,CAAA,EAAA,MAAA,IAAA,MAAA;AACA;AARA;AAzJA,CAAA","sourcesContent":["<template>\n  <v-container fluid>\n    <v-row no-gutters pa-0 ma-0>\n      <v-col sm=\"12\">\n        <search-bar @onSearch=\"onSearch\" @onReset=\"onReset\" />\n        <loading v-if=\"loadingview\" />\n        <empty-view v-if=\"emptyViewShow\" />\n        <v-row align=\"center\" dense>\n          <v-col v-for=\"(item, i) in items\" :key=\"i\" cols=\"12\">\n            <v-card @click=\"toDetail(item.id)\" class=\"pt-4\">\n              <v-card-title class=\"text-subtitle-2 mt-0 mb-0 ml-4 pa-0\">\n                {{ item.deptment }}\n                <v-spacer></v-spacer>\n                <v-card-actions class=\"pa-0\">\n                  <div\n                    class=\"mr-2 light-green--text text--accent-3 text-display-4\"\n                    v-if=\"item.articleType === '已发布'\"\n                  >\n                    <v-icon large color=\"light-green accent-3\"\n                      >mdi-circle-small</v-icon\n                    >{{ item.articleType }}\n                  </div>\n                  <div\n                    class=\"mr-2 red--text text--darken-3 text-display-4\"\n                    v-else-if=\"item.articleType === '已撤回'\"\n                  >\n                    <v-icon large color=\"red darken-3\">mdi-circle-small</v-icon>\n                    <a v-html=\"item.articleType\"></a>\n                  </div>\n                </v-card-actions>\n              </v-card-title>\n              <v-card-subtitle class=\"grey--text ma-0 pa-0 ml-4\">\n                {{ item.lastUpdateTime }}\n              </v-card-subtitle>\n              <div\n                class=\"\n                  d-flex\n                  flex-no-wrap\n                  justify-space-between\n                  black--text\n                  mt-2\n                \"\n              >\n                <div style=\"margin: 0; padding: 0\" class=\"ml-4\">\n                  <v-card-text\n                    class=\"title mt-0 pa-0\"\n                    v-html=\"$options.filters.lenghth(item.title, 60)\"\n                  ></v-card-text>\n                  <v-card-subtitle\n                    class=\"dense pa-0 pt-1 html-content\"\n                    v-html=\"$options.filters.lenghth(item.intro, 70)\"\n                  ></v-card-subtitle>\n                  <v-card-actions class=\"pa-0\">\n                    <v-row\n                      class=\"mt-2 mr-3 ml-0 pa-0 pb-3 caption grey--text\"\n                      align=\"center\"\n                    >\n                      <span\n                        v-if=\"item.attachNum\"\n                        class=\"mr-6 caption grey--text\"\n                        >附件({{ item.attachNum }})</span\n                      >\n                      <span class=\"mr-6 caption grey--text\"\n                        >阅读量({{ item.readNum }})</span\n                      >\n                      <span class=\"mr-6 caption grey--text\"\n                        >回复({{ item.replyNum }})</span\n                      >\n                      <span\n                        v-if=\"item.articleNo\"\n                        class=\"mr-6 caption grey--text\"\n                        >文章编号:{{ item.articleNo }}</span\n                      >\n                    </v-row>\n                  </v-card-actions>\n                </div>\n              </div>\n              <footer>\n                <v-row class=\"mt-2 mb-1 ml-1 pa-0 pb-3 caption\" align=\"center\">\n                  <v-dialog v-model=\"item.dlog2\" max-width=\"400\">\n                    <template v-slot:activator=\"{ on, attrs }\">\n                      <v-btn\n                        text\n                        depressed\n                        v-bind=\"attrs\"\n                        v-on=\"on\"\n                        class=\"ml-0\"\n                        color=\"primary\"\n                      >\n                        <v-icon left> mdi-arrow-left-top </v-icon>\n                        撤回\n                      </v-btn>\n                    </template>\n                    <v-card>\n                      <v-card-title justify-center>确认要撤回吗</v-card-title>\n                      <v-card-text\n                        >文章撤回后将从它的所有观看者列表中消失，确认要撤回吗？</v-card-text\n                      >\n                      <v-card-actions>\n                        <v-btn\n                          color=\"blue darken-1\"\n                          text\n                          @click=\"item.dlog2 = false\"\n                        >\n                          关闭\n                        </v-btn>\n                        <v-spacer></v-spacer>\n                        <v-btn\n                          color=\"blue darken-1\"\n                          text\n                          @click=\"withdraw(item, i)\"\n                        >\n                          确认\n                        </v-btn>\n                      </v-card-actions>\n                    </v-card>\n                  </v-dialog>\n                  <v-btn\n                    text\n                    depressed\n                    class=\"ml-0\"\n                    color=\"primary\"\n                    v-if=\"hasEditPermission\"\n                    @click.stop=\"saveAs(item.id)\"\n                  >\n                    <v-icon left> mdi-file-plus-outline </v-icon>\n                    另存为\n                  </v-btn>\n                </v-row>\n              </footer>\n            </v-card>\n          </v-col>\n        </v-row>\n      </v-col>\n    </v-row>\n    <page-loader @onLoadMore=\"onLoadMore\" />\n  </v-container>\n</template>\n\n<script>\nimport {\n  getMyArticle,\n  withdrawMessage,\n  collectionOperation,\n} from \"@/api/getData.js\";\nimport { ThrottleItem } from \"@/utils/tools.js\";\nimport { getMsgStatusDesc, highLightText } from \"@/utils/articleUtils.js\";\nimport { mapGetters } from \"vuex\";\nimport { aesEncrypt } from \"@/utils/encryption.js\";\nexport default {\n  components: {\n    searchBar: () => import(\"./articleList/searchBar.vue\"),\n    pageLoader: () => import(\"./articleList/pageLoader.vue\"),\n    loading: () => import(\"@/components/base/loading.vue\"),\n    emptyView: () => import(\"@/components/base/empty-view.vue\"),\n  },\n  data() {\n    return {\n      data: null,\n      loading: false,\n      loadingview: true,\n      emptyViewShow: false,\n      borderStatus:\n        \"border-left:4px solid blue !important;border-radius: 4px 4px 4px 4px\",\n      items: [],\n    };\n  },\n  computed: {\n    ...mapGetters({\n      hasEditPermission: \"user/hasEditPermission\",\n    }),\n  },\n  methods: {\n    onSearch(data) {\n      if (this.loading == true) return;\n      this.items = [];\n      this.data = data;\n      this.loadingview = true;\n      this.emptyViewShow = false;\n      this.searchArticles(this.data);\n    },\n    onReset(data) {\n      if (this.loading == true) return;\n      this.onSearch(data);\n    },\n\n    onLoadMore() {\n      if (this.loading == true) return;\n      this.searchArticles(this.data);\n    },\n    searchArticles(data) {\n      if (this.loading == true) return;\n      this.loading = true;\n      getMyArticle(data)\n        .then((res) => {\n          if (\n            res == null ||\n            typeof res == \"undefined\" ||\n            res.code == null ||\n            typeof res.code == \"undefined\" ||\n            res.code != 0 ||\n            res.data.messageList.length == 0\n          ) {\n            this.showEmptyViewIfNeeded();\n            return;\n          }\n          this.data.pageNum++;\n          let aTemp = [];\n          res.data.messageList.forEach((item) => {\n            let dTemp = {};\n            dTemp[\"id\"] = item.id;\n            dTemp[\"title\"] = item.title;\n            dTemp[\"intro\"] = item.content;\n            //部门\n            dTemp[\"deptment\"] = item.creatorDep;\n            //更新时间\n            dTemp[\"lastUpdateTime\"] = item.lastUpdateTime;\n            //文章状态:0:草稿 1:已发布 2:召回 3:已修改 4:已失效\n            dTemp[\"articleType\"] = getMsgStatusDesc(item.msgStatus);\n            //附件数量\n            dTemp[\"attachmentCount\"] = item.attachmentCount;\n            //回复数\n            dTemp[\"replyNum\"] = !item.feedbackCount ? 0 : item.feedbackCount;\n            //阅读数\n            dTemp[\"readNum\"] = !item.readAudienceCount\n              ? 0\n              : item.readAudienceCount;\n            //文件编号\n            dTemp[\"articleNo\"] = item.msgNumber;\n\n            aTemp.push(dTemp);\n          });\n          this.items = this.items.concat(aTemp);\n          highLightText(this.items, this.data.searchContent);\n        })\n        .catch((res) => {\n          console.log(res);\n        })\n        .finally(() => {\n          this.loading = false;\n          this.loadingview = false;\n        });\n    },\n    showEmptyViewIfNeeded() {\n      if (this.data.pageNum == 1) {\n        this.loading = false;\n        this.emptyViewShow = true;\n      }\n    },\n\n    //跳转详情页\n    toDetail(id) {\n      this.$router.push({\n        path: \"/artcleDetail\",\n        query: {\n          id: aesEncrypt(id + \"\"),\n          isMyPublish: true,\n        },\n      });\n    },\n    //跳转另存为发布文章\n    saveAs(id) {\n      this.$router.push({\n        name: \"publishArticle\",\n        params: { id: id },\n      });\n    },\n    //文章收藏\n    collect(index) {\n      //选择收藏的文章的指针\n      let item = this.items[index];\n      let type = Number(item.favoriteSelect);\n      let params = {\n        msgId: Number(item.id),\n        archiveId: type,\n      };\n      console.debug(\"收藏类型=======>\" + type);\n      item.favoriteSelect = null;\n\n      collectionOperation(params)\n        .then((res) => {\n          console.debug(\"修改文章类型\" + res);\n          this.items[index].favorite = type;\n        })\n        .catch()\n        .finally(() => (this.items[index].dlog = false));\n    },\n\n    withdraw: ThrottleItem(function (item, index) {\n      this.items[index].dlog2 = false;\n      withdrawMessage(item.id)\n        .then((res) => {\n          if ((!res && typeof res != \"undefined\") || res.code != 0) return;\n          var i = this.items.indexOf(item);\n          if (i >= 0) {\n            this.items.splice(i, 1);\n          }\n        })\n        .catch();\n    }),\n  },\n  //过滤文字\n  filters: {\n    lenghth(text, length, suffix = \"...\") {\n      if (!text) return;\n      text = text.replace(/<[^>]+>/g, \"\");\n      if (text.length <= length) {\n        return text;\n      }\n      return (text + \"\").replace(/\\n/g, \"<br/>\").substring(0, length) + suffix;\n    },\n  },\n};\n</script>\n<style>\n.html-content p {\n  margin-bottom: 0px;\n}\n</style>"],"sourceRoot":"src/views/page"}]}
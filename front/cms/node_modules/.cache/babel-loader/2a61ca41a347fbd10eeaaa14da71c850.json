A,CAAA,KAAA;AACA,SAtBA;AAuBA,QAAA,KAAA,CAAA,KAAA,GAAA,KAAA,CAAA,KAAA,CAAA,MAAA,CAAA,KAAA,CAAA;AACA,QAAA,aAAA,CAAA,KAAA,CAAA,KAAA,EAAA,KAAA,CAAA,IAAA,CAAA,aAAA,CAAA;AACA,OA9CA,EA+CA,KA/CA,CA+CA,UAAA,GAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,GAAA;;AACA,QAAA,KAAA,CAAA,qBAAA;AACA,OAlDA,EAmDA,OAnDA,CAmDA,YAAA;AACA,QAAA,KAAA,CAAA,OAAA,GAAA,KAAA;AACA,QAAA,KAAA,CAAA,WAAA,GAAA,KAAA;AACA,OAtDA;AAuDA,KA5EA;AA6EA,IAAA,qBA7EA,mCA6EA;AACA,UAAA,KAAA,IAAA,CAAA,OAAA,IAAA,CAAA,EAAA;AACA,aAAA,OAAA,GAAA,KAAA;AACA,aAAA,aAAA,GAAA,IAAA;AACA;AACA,KAlFA;AAoFA;AACA,IAAA,QArFA,oBAqFA,EArFA,EAqFA;AACA,WAAA,OAAA,CAAA,IAAA,CAAA;AACA,QAAA,IAAA,EAAA,eADA;AAEA,QAAA,KAAA,EAAA;AACA,UAAA,EAAA,EAAA,UAAA,CAAA,EAAA,GAAA,EAAA,CADA;AAEA,UAAA,WAAA,EAAA;AAFA;AAFA,OAAA;AAOA,KA7FA;AA8FA;AACA,IAAA,MA/FA,kBA+FA,EA/FA,EA+FA;AACA,WAAA,OAAA,CAAA,IAAA,CAAA;AACA,QAAA,IAAA,EAAA,gBADA;AAEA,QAAA,MAAA,EAAA;AAAA,UAAA,EAAA,EAAA;AAAA;AAFA,OAAA;AAIA,KApGA;AAqGA;AACA,IAAA,OAtGA,mBAsGA,KAtGA,EAsGA;AAAA;;AACA;AACA,UAAA,IAAA,GAAA,KAAA,KAAA,CAAA,KAAA,CAAA;AACA,UAAA,IAAA,GAAA,MAAA,CAAA,IAAA,CAAA,cAAA,CAAA;AACA,UAAA,MAAA,GAAA;AACA,QAAA,KAAA,EAAA,MAAA,CAAA,IAAA,CAAA,EAAA,CADA;AAEA,QAAA,SAAA,EAAA;AAFA,OAAA;AAIA,MAAA,OAAA,CAAA,KAAA,CAAA,iBAAA,IAAA;AACA,MAAA,IAAA,CAAA,cAAA,GAAA,IAAA;AAEA,MAAA,mBAAA,CAAA,MAAA,CAAA,CACA,IADA,CACA,UAAA,GAAA,EAAA;AACA,QAAA,OAAA,CAAA,KAAA,CAAA,WAAA,GAAA;AACA,QAAA,MAAA,CAAA,KAAA,CAAA,KAAA,EAAA,QAAA,GAAA,IAAA;AACA,OAJA,EAKA,KALA,GAMA,OANA,CAMA;AAAA,eAAA,MAAA,CAAA,KAAA,CAAA,KAAA,EAAA,IAAA,GAAA,KAAA;AAAA,OANA;AAOA,KAxHA;AA0HA,IAAA,QAAA,EAAA,YAAA,CAAA,UAAA,IAAA,EAAA,KAAA,EAAA;AAAA;;AACA,WAAA,KAAA,CAAA,KAAA,EAAA,KAAA,GAAA,KAAA;AACA,MAAA,eAAA,CAAA,IAAA,CAAA,EAAA,CAAA,CACA,IADA,CACA,UAAA,GAAA,EAAA;AACA,YAAA,CAAA,GAAA,IAAA,OAAA,GAAA,IAAA,WAAA,IAAA,GAAA,CAAA,IAAA,IAAA,CAAA,EAAA;;AACA,YAAA,CAAA,GAAA,MAAA,CAAA,KAAA,CAAA,OAAA,CAAA,IAAA,CAAA;;AACA,YAAA,CAAA,IAAA,CAAA,EAAA;AACA,UAAA,MAAA,CAAA,KAAA,CAAA,MAAA,CAAA,CAAA,EAAA,CAAA;AACA;AACA,OAPA,EAQA,KARA;AASA,KAXA;AA1HA,GAvBA;AA8JA;AACA,EAAA,OAAA,EAAA;AACA,IAAA,OADA,mBACA,IADA,EACA,MADA,EACA;AAAA,UAAA,MAAA,uEAAA,KAAA;AACA,UAAA,CAAA,IAAA,EAAA;;AACA,UAAA,IAAA,CAAA,MAAA,IAAA,MAAA,EAAA;AACA,eAAA,IAAA;AACA;;AACA,UAAA,aAAA,GAAA,oBAAA;;AACA,UAAA,IAAA,CAAA,OAAA,CAAA,aAAA,KAAA,CAAA,EAAA;AACA,QAAA,MAAA,IAAA,EAAA;AACA;;AAEA,aAAA,CAAA,IAAA,GAAA,EAAA,EAAA,SAAA,CAAA,CAAA,EAAA,MAAA,IAAA,MAAA;AACA;AAZA;AA/JA,CAAA","sourcesContent":["<template>\n  <v-container fluid>\n    <v-row no-gutters pa-0 ma-0>\n      <v-col sm=\"12\">\n        <search-bar\n          @onSearch=\"onSearch\"\n          @onReset=\"onReset\"\n          :departments=\"departments\"\n        />\n        <loading v-if=\"loadingview\" />\n        <empty-view v-if=\"emptyViewShow\" />\n        <v-row align=\"center\" dense>\n          <v-col v-for=\"(item, i) in items\" :key=\"i\" cols=\"12\">\n            <div @click=\"toDetail(item.id)\" class=\"card pt-4\">\n              <div class=\"mt-0 mb-0 ml-4 pa-0\" style=\"display: flex\">\n                <span class=\"deptment\">{{ item.deptment }}</span>\n                <v-spacer></v-spacer>\n                <v-img\n                  max-height=\"20\"\n                  max-width=\"57\"\n                  :src=\"item.articleTypeImg\"\n                >\n                  <template>\n                    <v-row\n                      class=\"fill-height ma-0\"\n                      align=\"center\"\n                      justify=\"center\"\n                    >\n                      <span class=\"article-type-text\">{{\n                        item.articleType\n                      }}</span>\n                    </v-row>\n                  </template>\n                </v-img>\n              </div>\n              <span class=\"article-time-text ma-0 pa-0 ml-4\">\n                {{ item.lastUpdateTime }}\n              </span>\n\n              <div class=\"pa-0 ma-0 ml-4 mt-2\">\n                <p\n                  class=\"article-title-text mt-0 pa-0\"\n                  :style=\"item.readStatus ? 'color:#1867C0' : ''\"\n                  v-html=\"$options.filters.lenghth(item.title, 50)\"\n                ></p>\n                <p\n                  class=\"article-intro-text pa-0 pt-1 html-content\"\n                  v-html=\"$options.filters.lenghth(item.intro, 90)\"\n                ></p>\n\n                <div class=\"article-report-text ma-0 mt-2 mr-3 ml-0 pa-0\">\n                  <span v-if=\"item.attachNum\" class=\"mr-6\"\n                    >附件({{ item.attachNum }})</span\n                  >\n                  <span class=\"mr-6\">阅读量({{ item.readCount }})</span>\n                  <span class=\"mr-6\">回复({{ item.feedbackCount }})</span>\n                  <span v-if=\"item.messageNember\" class=\"mr-6\"\n                    >文章编号:{{ item.messageNember }}</span\n                  >\n                </div>\n              </div>\n\n              <div class=\"mt-2 mb-1 ml-4 pa-0 pb-3 caption\">\n                <v-dialog v-model=\"item.dlog2\" max-width=\"400\">\n                  <template v-slot:activator=\"{ on, attrs }\">\n                    <button\n                      class=\"article-operator-text\"\n                      v-bind=\"attrs\"\n                      v-on=\"on\"\n                    >\n                      <v-icon class=\"article-operator-icon\">\n                        mdi-arrow-left-top\n                      </v-icon>\n                      撤回\n                    </button>\n                  </template>\n                  <v-card>\n                    <v-card-title justify-center>确认要撤回吗</v-card-title>\n                    <v-card-text\n                      >文章撤回后将从它的所有观看者列表中消失，确认要撤回吗？</v-card-text\n                    >\n                    <v-card-actions>\n                      <v-btn\n                        color=\"blue darken-1\"\n                        text\n                        @click=\"item.dlog2 = false\"\n                      >\n                        关闭\n                      </v-btn>\n                      <v-spacer></v-spacer>\n                      <v-btn\n                        color=\"blue darken-1\"\n                        text\n                        @click=\"withdraw(item, i)\"\n                      >\n                        确认\n                      </v-btn>\n                    </v-card-actions>\n                  </v-card>\n                </v-dialog>\n                <button\n                  class=\"article-operator-text ml-6\"\n                  v-if=\"hasEditPermission\"\n                  @click.stop=\"saveAs(item.id)\"\n                >\n                  <v-icon class=\"article-operator-icon\">\n                    mdi-file-plus-outline\n                  </v-icon>\n                  另存为\n                </button>\n              </div>\n            </div>\n          </v-col>\n        </v-row>\n      </v-col>\n    </v-row>\n    <page-loader @onLoadMore=\"onLoadMore\" />\n  </v-container>\n</template>\n\n<script>\nimport {\n  getMyArticle,\n  withdrawMessage,\n  collectionOperation,\n} from \"@/api/getData.js\";\nimport { ThrottleItem } from \"@/utils/tools.js\";\nimport {\n  getMsgStatusDesc,\n  highLightText,\n  getMsgStatusImg,\n  formatHtml,\n} from \"@/utils/articleUtils.js\";\nimport { mapGetters } from \"vuex\";\nimport { aesEncrypt } from \"@/utils/encryption.js\";\nexport default {\n  components: {\n    searchBar: () => import(\"./articleList/searchBar.vue\"),\n    pageLoader: () => import(\"./articleList/pageLoader.vue\"),\n    loading: () => import(\"@/components/base/loading.vue\"),\n    emptyView: () => import(\"@/components/base/empty-view.vue\"),\n  },\n  data() {\n    return {\n      data: null,\n      loading: false,\n      loadingview: true,\n      emptyViewShow: false,\n      departments: [\"全部\"],\n      borderStatus: \"border-left:3px solid #1867C0 !important;\",\n      items: [],\n    };\n  },\n  computed: {\n    ...mapGetters({\n      hasEditPermission: \"user/hasEditPermission\",\n    }),\n  },\n  methods: {\n    onSearch(data) {\n      if (this.loading == true) return;\n      this.items = [];\n      this.data = data;\n      this.loadingview = true;\n      this.emptyViewShow = false;\n      this.searchArticles(this.data);\n    },\n    onReset(data) {\n      if (this.loading == true) return;\n      this.onSearch(data);\n    },\n\n    onLoadMore() {\n      if (this.loading == true) return;\n      this.searchArticles(this.data);\n    },\n    searchArticles(data) {\n      if (this.loading == true) return;\n      this.loading = true;\n      getMyArticle(data)\n        .then((res) => {\n          if (\n            res == null ||\n            typeof res == \"undefined\" ||\n            res.code == null ||\n            typeof res.code == \"undefined\" ||\n            res.code != 0 ||\n            res.data.messageList.length == 0\n          ) {\n            this.showEmptyViewIfNeeded();\n            return;\n          }\n\n          if (this.data.pageNum == 1) {\n            let departments = [\"全部\"];\n            this.departments = departments.concat(res.data.departments);\n          }\n\n          this.data.pageNum++;\n          let aTemp = [];\n          res.data.messageList.forEach((item) => {\n            let dTemp = {};\n            dTemp[\"id\"] = item.id;\n            dTemp[\"title\"] = formatHtml(item.title);\n            dTemp[\"intro\"] = formatHtml(item.content);\n            //部门\n            dTemp[\"deptment\"] = item.creatorCompanyname;\n            //更新时间\n            dTemp[\"lastUpdateTime\"] = item.lastUpdateTime;\n            //文章状态:0:草稿 1:已发布 2:召回 3:已修改 4:已失效\n            dTemp[\"articleType\"] = getMsgStatusDesc(item.msgStatus);\n            dTemp[\"articleTypeImg\"] = getMsgStatusImg(item.msgStatus);\n            //附件数量\n            dTemp[\"attachmentCount\"] = item.attachmentCount;\n            //回复数\n            dTemp[\"feedbackCount\"] = !item.feedbackCount ? 0 : item.feedbackCount;\n            //阅读数\n            dTemp[\"readCount\"] = !item.readCount? 0: item.readCount;\n            //文件编号\n            dTemp[\"articleNo\"] = item.msgNumber;\n\n            aTemp.push(dTemp);\n          });\n          this.items = this.items.concat(aTemp);\n          highLightText(this.items, this.data.searchContent);\n        })\n        .catch((res) => {\n          console.log(res);\n          this.showEmptyViewIfNeeded();\n        })\n        .finally(() => {\n          this.loading = false;\n          this.loadingview = false;\n        });\n    },\n    showEmptyViewIfNeeded() {\n      if (this.data.pageNum == 1) {\n        this.loading = false;\n        this.emptyViewShow = true;\n      }\n    },\n\n    //跳转详情页\n    toDetail(id) {\n      this.$router.push({\n        path: \"/artcleDetail\",\n        query: {\n          id: aesEncrypt(id + \"\"),\n          isMyPublish: true,\n        },\n      });\n    },\n    //跳转另存为发布文章\n    saveAs(id) {\n      this.$router.push({\n        name: \"publishArticle\",\n        params: { id: id },\n      });\n    },\n    //文章收藏\n    collect(index) {\n      //选择收藏的文章的指针\n      let item = this.items[index];\n      let type = Number(item.favoriteSelect);\n      let params = {\n        msgId: Number(item.id),\n        archiveId: type,\n      };\n      console.debug(\"收藏类型=======>\" + type);\n      item.favoriteSelect = null;\n\n      collectionOperation(params)\n        .then((res) => {\n          console.debug(\"修改文章类型\" + res);\n          this.items[index].favorite = type;\n        })\n        .catch()\n        .finally(() => (this.items[index].dlog = false));\n    },\n\n    withdraw: ThrottleItem(function (item, index) {\n      this.items[index].dlog2 = false;\n      withdrawMessage(item.id)\n        .then((res) => {\n          if ((!res && typeof res != \"undefined\") || res.code != 0) return;\n          var i = this.items.indexOf(item);\n          if (i >= 0) {\n            this.items.splice(i, 1);\n          }\n        })\n        .catch();\n    }),\n  },\n  //过滤文字\n  filters: {\n    lenghth(text, length, suffix = \"...\") {\n      if (!text) return;\n      if (text.length <= length) {\n        return text;\n      }\n      let hightLightEle = '<font color=\"red\">';\n      if (text.indexOf(hightLightEle) >= 0) {\n        length += 25;\n      }\n\n      return (text + \"\").substring(0, length) + suffix;\n    },\n  },\n};\n</script>\n<style>\n.html-content p {\n  margin-bottom: 0px;\n}\n</style>"],"sourceRoot":"src/views/page"}]}